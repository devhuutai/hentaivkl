    <meta property="fb:app_id" content="" />
    
    <meta name="Author" content="HentaiVKL.net" />
    <meta name="copyright" content="<%=dataConfigsFooterCopy%>" />
    
    <meta name="thumbnail" content="<%=dataFormat?.thumb%>" />
